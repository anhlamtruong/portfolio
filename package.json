{
  "name": "cookit",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "build": "next build",
    "start": "next start",
    "lint": "next lint",
    "reset:prisma-mysql": "npx prisma migrate reset --schema=./prisma/mysql/schema.prisma",
    "generate:prisma-mysql": "npx prisma generate --schema=./prisma/mysql/schema.prisma",
    "reset:prisma-mongo": "npx prisma migrate reset --schema=./prisma/mongodb/schema.prisma",
    "generate:prisma-mongo": "npx prisma generate --schema=./prisma/mongodb/schema.prisma",
    "generate:prisma-all": "npm run generate:prisma-mysql && npm run generate:prisma-mongo",
    "push:prisma-mysql": "npx prisma db push --schema=./prisma/mysql/schema.prisma",
    "push:prisma-mongo": "npx prisma db push --schema=./prisma/mongodb/schema.prisma",
    "push:prisma-all": "npm run push:prisma-mysql && npm run push:prisma-mongo",
    "generate": "npm run generate:prisma-all && npm run push:prisma-all && next dev",
    "dev": "next dev"
  },
  "dependencies": {
    "@emotion/react": "^11.11.1",
    "@emotion/styled": "^11.11.0",
    "@hookform/resolvers": "^3.3.2",
    "@next-auth/prisma-adapter": "^1.0.7",
    "@prisma/client": "^5.6.0",
    "@radix-ui/react-avatar": "^1.0.4",
    "@radix-ui/react-dialog": "^1.0.5",
    "@radix-ui/react-hover-card": "^1.0.7",
    "@radix-ui/react-label": "^2.0.2",
    "@radix-ui/react-slot": "^1.0.2",
    "@tailwindcss/forms": "^0.5.6",
    "@types/node": "20.8.7",
    "@types/react": "18.2.29",
    "@types/react-dom": "18.2.13",
    "autoprefixer": "10.4.16",
    "axios": "^1.6.0",
    "bcrypt": "^5.1.1",
    "class-variance-authority": "^0.7.0",
    "clsx": "^2.0.0",
    "eslint": "8.51.0",
    "eslint-config-next": "^14.0.3",
    "framer-motion": "^10.16.4",
    "lucide-react": "^0.288.0",
    "next": "^14.0.3",
    "next-auth": "^4.24.4",
    "next-cloudinary": "^5.7.0",
    "postcss": "8.4.31",
